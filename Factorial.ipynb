    "# Python program to fina factorial of a number\n",
    "fact=1\n",
    "num=int(input(\"Enter a number:\"))\n",
    "org=num\n",
    "while num>0:\n",
    "    fact=fact*num\n",
    "    num=num-1\n",
    "print(\"The Factorial of \",org,\"is\",fact)"
