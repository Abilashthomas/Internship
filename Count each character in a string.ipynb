    "#python program to print the frequency of each of the characters present in a given string \n",
    "string=input(\"Enter a string:\")\n",
    "print(\"String is:\",string)\n",
    "count={}\n",
    "for x in string:\n",
    "    if x in count.keys():\n",
    "        count[x]+=1\n",
    "    else:\n",
    "            count[x]=1\n",
    "print(count)"
